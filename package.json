{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cd": "gatsby clean && gatsby develop",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.1",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.4",
    "@chakra-ui/theme-tools": "^1.1.7",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "email-octopus": "^1.1.2",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.11.1",
    "gatsby-cli": "^3.13.0",
    "gatsby-plugin-feed": "^3.9.0",
    "gatsby-plugin-google-gtag": "^3.10.0",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-mdx": "^2.8.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webfonts": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^4.5.0",
    "gatsby-remark-images": "^5.6.0",
    "gatsby-remark-relative-images": "0.2.3",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-airtable": "^2.2.1",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-source-rss-feed": "^1.2.2",
    "gatsby-source-spotify": "^1.3.2",
    "gatsby-transformer-sharp": "^3.9.0",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/node": "14.17.1",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-plugin-root-import": "6.6.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "process-top": "^1.2.0",
    "typescript": "4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
